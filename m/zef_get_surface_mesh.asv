[zef.file zef.file_path zef.surface_mesh_type] = zef_import_surface_mesh_type; 
if not(isequal(zef.file,0));
[zef.aux_points,zef.aux_triangles,zef.aux_submesh_ind] = zef_get_mesh([zef.file_path zef.file], zef.current_tag, zef.surface_mesh_type,'full'); 
end; 
evalin('base',['zef.' zef.current_tag '_points = ' num2str(zef.aux_points(:)') ';']);
evalin('base',['zef.' zef.current_tag '_triangles = ' num2str(zef.aux_triangles(:)') ';']);
evalin('base',['zef.' zef.current_tag '_submesh_ind = ' num2str(zef.aux_submesh_ind) ';']);

evalin('base',['zef.' zef.current_tag '_points = reshape('zef.' zef.current_tag '_points, length(' 'zef.' zef.current_tag '_points)/3,']